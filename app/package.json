{
  "name": "ff-app",
  "version": "0.1.0",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-measure": "^2.1.0",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.17.0",
    "react-toastify": "^4.2.0",
    "react-toasts": "^2.0.13",
    "react-tooltip": "^3.6.1",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id E2UNBWCKDABKA6 --paths \"/*\" --profile personal",
    "deploy:s3": "aws s3 sync ./build s3://www.ffdraft.app --acl public-read --sse --delete --cache-control max-age=604800,public --profile personal",
    "deploy:s3:personal": "aws s3 sync ./build s3://www.fantasydraft.joshuatimmons.com --acl public-read --sse --delete --cache-control max-age=604800,public --profile personal",
    "deploy": "npm run build && npm run deploy:s3 && npm run deploy:s3:personal && npm run deploy:cloudfront"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react-measure": "^2.0.4",
    "@types/node": "^10.5.8",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-tooltip": "^3.3.6",
    "@types/react-virtualized": "^9.18.6",
    "typescript": "^3.0.1"
  }
}
