{
  "name": "ff-app",
  "version": "0.1.0",
  "repository": "https://github.com/JJTimmons/ff",
  "dependencies": {
    "react": "^16.8.6",
    "react-dnd": "^8.0.3",
    "react-dnd-html5-backend": "^8.0.3",
    "react-dom": "^16.8.6",
    "react-measure": "^2.3.0",
    "react-redux": "^5.1.1",
    "react-scripts-ts": "^4.0.8",
    "react-toastify": "^4.5.2",
    "react-toasts": "^2.0.14",
    "react-tooltip": "^3.10.0",
    "react-virtualized": "^9.21.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id E2UNBWCKDABKA6 --paths \"/*\" --profile personal",
    "deploy:s3": "aws s3 sync ./build s3://www.ffdraft.app --acl public-read --sse --delete --cache-control max-age=10800,public --profile personal",
    "deploy": "npm run build && npm run deploy:s3 && npm run deploy:cloudfront"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-measure": "^2.0.5",
    "@types/react-redux": "^6.0.14",
    "@types/react-tooltip": "^3.9.2",
    "@types/react-virtualized": "^9.21.2",
    "jest": "^23.6.0",
    "typescript": "^3.5.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
